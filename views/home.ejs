	    <script>
	    const s0 = ( sketch ) => {

	    	//let dashDist = [5, 5];

			let x;
			let y;
			let px;
			let py;

			let magnitude = 25;

			// let windowWidth = 1200;
			// let windowHeight = 1000;

			let windowWidth = sketch.displayWidth;
			let windowHeight = sketch.displayHeight;

			let bounds = {
				"width": 400,
				"height": 200
			}
			let canvas;
		    		
	    	sketch.setup = () => {
	    		canvas = sketch.createCanvas(windowWidth, windowHeight);

	    		canvas.position(0, 0);
			  	canvas.style('z-index',  '-1');

	    		sketch.background(255);
				sketch.stroke(0, 0, 255);
				sketch.strokeWeight(45);
				
				x = windowWidth/2;
				y = windowHeight/2;

				//sketch.rect(windowWidth/2 - bounds.width/2, windowHeight/2 - bounds.height/2, bounds.width, bounds.height);

				// sketch.strokeWeight(2);
				// sketch.textSize(50);
				// sketch.text('test', 200, 200);
				// sketch.strokeWeight(25);

				// console.log(sketch.floor(sketch.random(0,100)));

				// Canvas boundaries
				//sketch.drawingContext.setLineDash(dashDist);
				//sketch.rect(0, 0, sketch.width, sketch.height);
				// Reset to non-dashed line
				//sketch.drawingContext.setLineDash([0, 0]);
	    	}

	    	sketch.draw = () => {
	    		// opacity += increase;

	    		// sketch.stroke(0, 0, 255, opacity);

	    		let offset = sketch.floor(sketch.random(0,100));

			    px = x;
				py = y;

				x += sketch.random(magnitude * -1, magnitude);
				y += sketch.random(magnitude * -1, magnitude);

				sketch.line(px, py, x, y);
				sketch.line(px - offset, py - offset, x - offset, y - offset);
				sketch.line(px + offset, py + offset, x + offset, y + offset);

				// sketch.line(px, py, x * -1, y * -1);

				// sketch.line(px +20, py +20, x+10, y+10);

				//sketch.ellipse(sketch.mouseX, sketch.mouseY, 20, 20);

				// sketch.line(sketch.floor(sketch.random(0,800)), sketch.floor(sketch.random(0,800)), x, y);

				// Keep the walker inside the fence
				if(x >= windowWidth/2 + bounds.width/2){
					x -= (magnitude * 2); 
				} else if(x <= windowWidth/2 - bounds.width/2){
					x += (magnitude * 2);
				}


				if(y >= windowHeight/2 + bounds.height/2){
					y -= (magnitude * 2); 
				} else if(y <= windowHeight/2 - bounds.height/2){
					y += (magnitude * 2);
				}

				
			};

			sketch.mousePressed = () => {
				
				x = sketch.mouseX;
				y = sketch.mouseY;
				//sketch.stroke(sketch.random(25, 150), sketch.random(25, 150), 200);
			};

			sketch.mouseDragged = () => {
				sketch.line(sketch.pmouseX, sketch.pmouseY, sketch.mouseX, sketch.mouseY);
			};

			// sketch.windowResized = () => {
			// 	sketch.resizeCanvas(sketch.windowWidth, sketch.windowHeight);
			// };


			
	    };

	    //let myp5_0 = new p5(s0);
	    let myp5_0 = new p5(s0, 'p5sketch0');

	    </script>

