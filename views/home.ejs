	    <script>
	    const s0 = ( sketch ) => {

	    	//let dashDist = [5, 5];

			let x;
			let y;
			let px;
			let py;
			let magnitude = 15;
			let clicked = false;
			// let opacity = 0;
			// let increase = 10;
			let canvas;
		    		
	    	sketch.setup = () => {
	    		canvas = sketch.createCanvas(300, 200);

	    		canvas.position(0, 0);
			  	canvas.style('z-index',  '-1');

	    		sketch.background(255);
				sketch.stroke(0, 0, 255);
				sketch.strokeWeight(25);
				
				x = sketch.width/2;
				y = sketch.height/2;
				x=0;
				y=0;

				console.log(sketch.floor(sketch.random(0,100)));

				// Canvas boundaries
				//sketch.drawingContext.setLineDash(dashDist);
				//sketch.rect(0, 0, sketch.width, sketch.height);
				// Reset to non-dashed line
				//sketch.drawingContext.setLineDash([0, 0]);
	    	}

	    	sketch.draw = () => {
	    		// opacity += increase;

	    		// sketch.stroke(0, 0, 255, opacity);

			    px = x;
				py = y;

				x += sketch.random(magnitude * -1, magnitude);
				y += sketch.random(magnitude * -1, magnitude);

				sketch.line(px, py, x, y);

				// sketch.line(px, py, x * -1, y * -1);

				// sketch.line(px +20, py +20, x+10, y+10);

				//sketch.ellipse(sketch.mouseX, sketch.mouseY, 20, 20);

				sketch.line(sketch.pmouseX, sketch.pmouseY, sketch.mouseX, sketch.mouseY);

				// sketch.line(sketch.floor(sketch.random(0,800)), sketch.floor(sketch.random(0,800)), x, y);

				if(x >= sketch.width){
					x -= (magnitude *2); 
				} else if(x <= 0){
					x += (magnitude * 2);
				}


				if(y >= sketch.height){
					y -= (magnitude *2); 
				} else if(y <= 0){
					y += (magnitude * 2);
				}

				
			};

			sketch.mousePressed = () => {
				
				x = sketch.mouseX;
				y = sketch.mouseY;
				//sketch.stroke(sketch.random(25, 150), sketch.random(25, 150), 200);
			}


			
	    };

	    //let myp5_0 = new p5(s0);
	    let myp5_0 = new p5(s0, 'p5sketch0');

	    </script>

